# include<stdio.h>
# include <stdlib.h>
# include <string.h>



float calcularMedia (int numeros[], int tamanho){
	int soma = 0;
	int i = 0;
	for (i = 0; i < tamanho; i++){
		soma += numeros[1];
		
	}
	return (float) soma / tamanho;
}

int main (){
	int lista [5];
	int tamanho = sizeof(lista) / sizeof(lista[0]);
	float media = calcularMedia(lista, tamanho);
	printf("A media dos numeros e: %.2lf\n", media);
	 
	 return 0;
}
